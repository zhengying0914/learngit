<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
		
<mapper namespace="GoodsTypeMapper">
	<select id="selectAll" resultType="com.sxsys.entity.GoodsType">
	SELECT * FROM goodstype
	</select>
	
	<select id="selectById" parameterType="int" resultType="com.sxsys.entity.GoodsType">
	SELECT * FROM goodstype where id=#{_parameter}
	</select>
	
</mapper>